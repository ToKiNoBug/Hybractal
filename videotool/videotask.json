{
    "common": {
        "rows": 1080,
        "cols": 1920,
        "hybf-prefix": "./compute/", //optional
        "png-prefix": "./png/", //optional
        "video-prefix": "./video/", //optional
        "maxit": 4096,
        "frame-num": 14,
        "ratio": 2
    },
    "compute": {
        "centerhex": "0x8d9aef6df402d03fafb69a745266eabf",
        "y-span": 4,
        // x-span is optional
        "threads": 20
    },
    "render": {
        "png-per-frame": 60,
        "extra-png-num": 4,
        "config-file": "render1.json",
        "threads": 20
    },
    "makevideo": {
        "ffmpeg-exe": "ffmpeg", //optional
        "itermediate-config": {
            "extension": ".mp4", //optional
            "encoder": "libx264", //optional
            "encoder-flags": "-crf 10" //optional, default ""
        },
        "product-config": {
            "extension": ".mp4", //optional
            "encoder": "libx265", //optional
            "encoder-flags": "-crf 18" //optional
        },
        "product-name": "product",
        "threads": 4 //optional
    }
}